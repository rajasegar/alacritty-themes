{
  "name": "alacritty-themes",
  "version": "4.1.3",
  "description": "Themes for Alacritty : A cross-platform GPU-Accelerated Terminal emulator",
  "main": "index.js",
  "bin": {
    "alacritty-themes": "./bin/cli.js"
  },
  "scripts": {
    "test": "mocha --recursive",
    "deploy": "git push && git push --tags && npm publish",
    "lint": "eslint .",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "alacritty",
    "theme",
    "themes",
    "terminal emulators",
    "terminal"
  ],
  "author": "Rajasegar Chandran <rajasegar.c@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "prompts": "^2.4.0",
    "temp": "^0.9.4",
    "yaml": "^1.10.0"
  },
  "preferGlobal": true,
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^7.0.2",
    "mocha": "^8.4.0",
    "mock-fs": "^5.0.0",
    "prettier": "^2.2.1",
    "semantic-release": "^17.4.5",
    "sinon": "^11.1.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "release(version): Release ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
